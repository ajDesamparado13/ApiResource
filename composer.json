{
  "name": "freedom/api-resource",
  "description": "Yet another package for building laravel api implementation",
  "type": "library",
  "require-dev": {
    "prettus/l5-repository": "dev-master"
  },
  "require": {
    "illuminate/http": "~5.0|~6.0|~7.0",
    "illuminate/config": "~5.0|~6.0|~7.0",
    "illuminate/support": "~5.0|~6.0|~7.0",
    "illuminate/database": "~5.0|~6.0|~7.0",
    "illuminate/queue": "~5.0|~6.0|~7.0",
    "illuminate/broadcasting": "~5.0|~6.0|~7.0",
    "illuminate/events": "~5.0|~6.0|~7.0",
    "illuminate/routing": "~5.0|~6.0|~7.0",
    "illuminate/auth": "~5.0|~6.0|~7.0",
    "illuminate/contracts": "~5.0|~6.0|~7.0"
  },
  "authors": [
    {
      "name": "Allen Jay Desamparado",
      "email": "ajDesamparado13@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Freedom\\ApiResource\\": "src/ApiResource/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Freedom\\ApiResource\\Providers\\LaravelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
